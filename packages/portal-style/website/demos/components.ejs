<%- include('_partials/_head.ejs'); %>

<body>
  <div class="dp-po">
    <main class="dp-po-main dp-po-components">
      <div class="container">
        <h1>Global Components</h1>
        <hr />
        <h2>Header</h2>
        <%- include('_partials/_header.ejs', { loggedIn: false }); %>
        <hr />
        <h2>Search</h2>
        <%- include('_partials/_search.ejs'); %>
        <hr />
        <h2>Breadcrumbs</h2>
        <%- include('_partials/_breadcrumbs.ejs'); %>
        <hr />
        <h2>Subscribe</h2>
        <div class="row">
          <div class="col-sm-3">
            <%- include('_partials/_subscribe.ejs'); %>
          </div>
          <div class="col-sm-3">
            <%- include('_partials/_subscribe-1.ejs'); %>
          </div>
        </div>
        <hr />
        <h2>Pagination</h2>
        <%- include('_partials/_pagination.ejs'); %>
        <hr />
        <h2>Login Form</h2>
        <div class="row">
          <div class="col-sm-3">
            <%- include('_partials/_form-login.ejs'); %>
          </div>
        </div>
        <hr />
        <h2>Social Login Form</h2>
        <div class="row">
          <div class="col-sm-3">
            <%- include('_partials/_form-login-social.ejs'); %>
          </div>
        </div>
        <hr />
        <h2>Registration Form</h2>
        <div class="row">
          <div class="col-sm-3">
            <%- include('_partials/_form-register.ejs'); %>
          </div>
        </div>
        <hr />
        <div class="row">
          <button type="button" class="btn btn-primary">Primary</button>
          <button type="button" class="btn btn-secondary">Secondary</button>
          <button type="button" class="btn btn-success">Success</button>
          <button type="button" class="btn btn-danger">Danger</button>
          <button type="button" class="btn btn-warning">Warning</button>
          <button type="button" class="btn btn-info">Info</button>
          <button type="button" class="btn btn-light">Light</button>
          <button type="button" class="btn btn-dark">Dark</button>

          <button type="button" class="btn btn-link">Link</button>
        </div>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
</body>

</html>
