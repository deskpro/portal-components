<div class="dp-po">
  <header class="dp-po-header">
    <div class="container-fluid">
      <div class="row">
        <div class="col-3 col-sm-6">
          <div class="dp-po-brand-left">
            <a href="/portal-components/portal-style/" class="dp-po-homelink">
              <span class="dp-po-logo">
                <img src="/portal-components/portal-style/static/img/brand/logo.svg" />
              </span>
              <span class="dp-po-brand-name">Support Center</span>
            </a>
          </div>
        </div>
        <div class="col-9 col-sm-6">
          <div class="dp-po-header-right">
            <div class="dp-po-language">
              <a class="dp-po-language-link" href="" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <img src="/portal-components/portal-style/static/img/docs/flag-uk.jpg" /> English
                <i class="dp-po-icon far fa-angle-down"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-left">
                <a class="dropdown-item" href="#">
                  <img class="dp-po-icon" src="/portal-components/portal-style/static/img/docs/flag-germany.jpg" />
                  German
                </a>
                <a class="dropdown-item" href="#">
                  <img class="dp-po-icon" src="/portal-components/portal-style/static/img/docs/flag-france.jpg" />
                  French
                </a>
              </div>
            </div>
            <div class="dp-po-header-button">
              <a href="javascript:void(0)" class="btn btn-primary" title="Contact Us">Contact us</a>
            </div>
            <div class="dp-po-user">
              <% if (typeof loggedIn !== 'undefined') { %>
              <a href="javascript:void(0)" class="dp-po-user-login dp-po-avatar" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">
                <img src="/portal-components/portal-style/static/img/docs/profile.png" class="dp-po-avatar-image" />
                <i class="dp-po-icon far fa-angle-down"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-right dp-po-user-dropdown dp-po-user-menu">
                <div class="dp-po-avatar">
                  <img src="/portal-components/portal-style/static/img/docs/profile.png" /> Artem
                  Berdyshev
                </div>
                <ul class="dp-po-user-menu-list">
                  <li class="dp-po-user-menu-item">
                    <a href="#" class="dp-po-user-menu-link"><img class="dp-profile-svg dp-po-user-menu-image"
                        src="/portal-components/portal-style/static/img/profile-icons/profile.svg">View profile</a>
                  </li>
                  <li class="dp-po-user-menu-item">
                    <a class="dp-po-user-menu-link" href="#"><img class="dp-profile-svg dp-po-user-menu-image"
                        src="/portal-components/portal-style/static/img/profile-icons/tickets.svg">My tickets</a>
                  </li>
                  <li class="dp-po-user-menu-item">
                    <a class="dp-po-user-menu-link" href="#"><img class="dp-profile-svg dp-po-user-menu-image"
                        src="/portal-components/portal-style/static/img/profile-icons/chats.svg">My chats</a>
                  </li>
                </ul>
                <a href="#" id="top-logout-btn" class="btn btn-primary btn-block">Logout</a>
              </div>
              <% } else { %>
              <a href="javascript:void(0)" class="dp-po-user-login" data-toggle="dropdown" aria-haspopup="true"
                aria-expanded="false">Login</a>
              <div class="dropdown-menu dropdown-menu-right dp-po-user-dropdown">
                <h4 class="dropdown-item-text dp-po-user-dropdown-title">New to Deskpro? <a href="">Register for an
                    account</a></h4>
                <%- include('_form-login.ejs'); %>
                <%- include('_form-login-social.ejs'); %>
              </div>
              <% } %>
            </div>
          </div>

        </div>
      </div>
    </div>
  </header>
</div>
