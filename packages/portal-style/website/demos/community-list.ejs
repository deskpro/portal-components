<%- include('_partials/_head.ejs'); %>

<body>
  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-community-home">
      <div class="container">
        <div class="row">
          <div class="col-sm-5">
            <div class="dp-po-community-title">
              <a class="dp-po-community-title-link" href="" data-toggle="dropdown" aria-haspopup="true"
                aria-expanded="false">
                <span class="dp-po-community-title-icon dp-po-community-title-icon-success">
                  <i class="dp-po-icon dp-po-icon-success fad fa-comment-alt-check"></i>
                </span>
                Suggestions
                <i class="dp-po-icon dp-po-community-title-arrow far fa-angle-down"></i>
              </a>
              <div class="dropdown-menu dropdown-menu-left">
                <a class="dropdown-item" href="#">
                  <i class="dp-po-icon dp-po-icon-danger fad fa-hands-helping"></i> Help
                </a>
                <a class="dropdown-item" href="#">
                  <i class="dp-po-icon dp-po-icon-success fad fa-comment-alt-check"></i> Suggestions
                </a>
                <a class="dropdown-item" href="#">
                  <i class="dp-po-icon dp-po-icon-pink fad fa-comments"></i> Chat
                </a>
                <a class="dropdown-item" href="#">
                  <i class="dp-po-icon dp-po-icon-warning fad fa-code"></i> Developers
                </a>
                <a class="dropdown-item" href="#">
                  <i class="dp-po-icon dp-po-icon-info fad fa-receipt"></i> Finance
                </a>
                <a class="dropdown-item" href="#">
                  <i class="dp-po-icon dp-po-icon-neutral fad fa-rocket"></i> General chat
                </a>
              </div>
            </div>
          </div>
          <div class="col-sm-4">
            <div class="d-none d-sm-block">
              <%- include('_partials/_community-button.ejs'); %>
            </div>
          </div>
          <div class="col-sm-3">
            <div class="d-none d-sm-block">
              <%- include('_partials/_subscribe.ejs'); %>
            </div>
          </div>
        </div>
        <div class="d-block d-sm-none">
          <div class="dp-po-post-mobile-area">
            <%- include('_partials/_subscribe.ejs'); %>
            <%- include('_partials/_community-button.ejs'); %>
          </div>
        </div>


        <%- include('_partials/_community-header-mobile.ejs'); %>
        <div class="row">
          <div class="col-sm-12">
            <%- include('_partials/_community-header.ejs'); %>
            <div class="dp-po-section dp-po-block">
              <%- include('_partials/_community-topics-list.ejs'); %>
            </div>
            <%- include('_partials/_pagination.ejs'); %>
          </div>
        </div>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
</body>

</html>
