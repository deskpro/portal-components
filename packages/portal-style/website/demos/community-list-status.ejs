<%- include('_partials/_head.ejs'); %>

<body>
  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-community-home">
      <div class="container">
        <div class="row">
          <div class="col-sm-5">
            <div class="dp-po-title">
              <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/community.svg">
              <h2 class="dp-po-title-text">Community</h2>
            </div>
          </div>
          <div class="col-sm-4">
            <div class="d-none d-sm-block">
              <%- include('_partials/_community-button.ejs'); %>
            </div>
          </div>
          <div class="col-sm-3">
            <div class="d-none d-sm-block">
              <%- include('_partials/_subscribe.ejs'); %>
            </div>
          </div>
        </div>
        <div class="d-block d-sm-none">
          <div class="dp-po-post-mobile-area">
            <%- include('_partials/_subscribe.ejs'); %>
            <%- include('_partials/_community-button.ejs'); %>
          </div>
        </div>


        <%- include('_partials/_community-header-mobile.ejs'); %>
        <div class="row">
          <div class="col-sm-12">
            <%- include('_partials/_community-header.ejs'); %>
            <div class="dp-po-section dp-po-block">
              <%- include('_partials/_community-topics-list-status.ejs'); %>
            </div>
            <%- include('_partials/_pagination.ejs'); %>
          </div>
        </div>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
