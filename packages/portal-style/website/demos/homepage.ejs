<%- include('_partials/_head.ejs'); %>

<body>

  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>
  <%- include('_partials/_notification.ejs'); %>
  <div class="dp-po">
    <main class="dp-po-main dp-po-homepage">
      <div class="container">
        <%- include('_partials/_navigation-block.ejs'); %>

        <!-- News-->
        <div class="dp-po-homepage-news">
          <div class="row">
            <div class="col-sm-6">
              <div class="dp-po-title">
                <a href="" class="dp-po-title-link">
                  <img class="dp-po-title-svg svg" src="/portal-components/portal-style/static/img/page-icons/news.svg">
                  <h2 class="dp-po-title-text">News</h2>
                </a>
              </div>

            </div>
            <div class="col-sm-6 d-flex justify-content-end">
              <%- include('_partials/_subscribe-1.ejs'); %>
            </div>
          </div>

          <div class="dp-po-block">
            <%- include('_partials/_news-carousel.ejs'); %>
          </div>
        </div>

        <!-- Knowledgebase Variation 1-->
        <div class="dp-po-homepage-kb">
          <div class="dp-po-title">
            <a href="" class="dp-po-title-link">
              <img class="dp-po-title-svg"
                src="/portal-components/portal-style/static/img/page-icons/knowledgebase.svg">
              <h2 class="dp-po-title-text">Knowledgebase</h2>
            </a>
          </div>
          <div class="dp-po-block">
            <%- include('_partials/_kb-featured.ejs'); %>
            <%- include('_partials/_kb-categories-title.ejs'); %>
          </div>
        </div>

        <!-- Knowledgebase Variation 2-->
        <div class="dp-po-homepage-kb">
          <div class="dp-po-title">
            <a href="" class="dp-po-title-link">
              <img class="dp-po-title-svg"
                src="/portal-components/portal-style/static/img/page-icons/knowledgebase.svg">
              <h2 class="dp-po-title-text">Knowledgebase</h2>
            </a>
          </div>
          <div class="dp-po-block">
            <div class="row mb-4">
              <div class="col-sm-6 ">
                <%- include('_partials/_kb-categories.ejs'); %>
              </div>
              <div class="col-sm-6">
                <%- include('_partials/_kb-categories.ejs'); %>
              </div>
            </div>
            <div class="row">
              <div class="col-sm-6">
                <%- include('_partials/_kb-categories.ejs'); %>
              </div>
              <div class="col-sm-6">
                <%- include('_partials/_kb-categories.ejs'); %>
              </div>
            </div>
          </div>
        </div>

        <!-- Community Variation 1-->
        <div class="dp-po-homepage-community">
          <div class="row align-items-center">
            <div class="col-sm-3">
              <div class="dp-po-title">
                <a href="" class="dp-po-title-link">
                  <img class="dp-po-title-svg"
                    src="/portal-components/portal-style/static/img/page-icons/community.svg">
                  <h2 class="dp-po-title-text">Community</h2>
                </a>
              </div>
              <div class="dp-po-mobile-community-tags">
                <a href="javascript:void(0)" class="dp-po-mobile-community-tags-link"> <i
                    class="dp-po-icon fad fa-hashtag"></i> Forums</a>
              </div>
            </div>
            <div class="col-sm-9">
              <%- include('_partials/_community-channel-tags.ejs'); %>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-6">
              <div class="dp-po-block">
                <%- include('_partials/_community-featured.ejs'); %>
              </div>
            </div>
            <div class="col-sm-6">
              <div class="dp-po-block">
                <%- include('_partials/_community-status.ejs'); %>
              </div>
            </div>
          </div>
        </div>

        <!-- Community Variation 2-->
        <div class="dp-po-homepage-community">
          <div class="dp-po-title">
            <a href="" class="dp-po-title-link">
              <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/community.svg">
              <h2 class="dp-po-title-text">Community</h2>
            </a>
          </div>
          <%- include('_partials/_community-channel.ejs'); %>
        </div>

        <!-- Community Variation 3-->
        <div class="dp-po-homepage-community">
          <div class="dp-po-title">
            <a href="" class="dp-po-title-link">
              <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/community.svg">
              <h2 class="dp-po-title-text">Community</h2>
            </a>
          </div>
          <%- include('_partials/_community-channel-blocks.ejs'); %>
        </div>

        <!-- Guides-->
        <div class="dp-po-homepage-guides">
          <div class="dp-po-title">
            <a href="" class="dp-po-title-link">
              <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/guides.svg">
              <h2 class="dp-po-title-text">Guides</h2>
            </a>
          </div>
          <%- include('_partials/_guides.ejs'); %>
        </div>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>

</body>

</html>
