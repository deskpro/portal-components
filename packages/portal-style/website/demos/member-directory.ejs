<%- include('_partials/_head.ejs'); %>

<body>
  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-member-directory">
      <div class="container">
        <div class="dp-po-title">
          <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/direct-message.svg">
          <h2 class="dp-po-title-text">Member Directory</h2>
        </div>
        <div class="dp-po-section dp-po-block">
          <div class="row">
            <div class="col-10 col-sm-6">
              <form class="dp-po-member-search">
                <input type="text" name="" placeholder="Search"></input>
                <button type="submit"> <i class="dp-po-icon far fa-search"></i></button>
              </form>
            </div>
            <div class="col-2 col-sm-6 text-right">
              <div class="dp-po-columnfilter">
                <a href="javascript:void(0)" class="dp-po-columnfilter-link" data-toggle="dropdown" aria-haspopup="true"
                  aria-expanded="false"><i class="dp-po-icon far fa-ballot-check"></i></a>
                <div class="dropdown-menu dropdown-menu-right dp-po-columnfilter-dropdown">
                  <ul class="dp-po-columnfilter-list">
                    <li class="dp-po-columnfilter-item">
                      <div class="form-group dp-po-form-check">
                        <input type="checkbox" class="form-check-input" id="SubjectCheck">
                        <label class="form-check-label" for="SubjectCheck">Email</label>
                      </div>
                    </li>
                    <li class="dp-po-columnfilter-item">
                      <div class="form-group dp-po-form-check">
                        <input type="checkbox" class="form-check-input" id="DepartmentCheck" checked>
                        <label class="form-check-label" for="DepartmentCheck">Phone Number</label>
                      </div>
                    </li>
                    <li class="dp-po-columnfilter-item">
                      <div class="form-group dp-po-form-check">
                        <input type="checkbox" class="form-check-input" id="UserCheck">
                        <label class="form-check-label" for="UserCheck">Job Title</label>
                      </div>
                    </li>
                    <li class="dp-po-columnfilter-item">
                      <div class="form-group dp-po-form-check">
                        <input type="checkbox" class="form-check-input" id="agentCheck">
                        <label class="form-check-label" for="agentCheck">Company</label>
                      </div>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
          </div>

          <div class="dp-po-table dp-po-table-col5 dp-po-table-member">
            <div class="dp-po-table-row dp-po-table-row-head">
              <span class="dp-po-table-cell dp-po-table-cell-large">Name
                <div class="dropdown">
                  <a href="#" role="button" id="dropdownMenuLink" data-toggle="dropdown" aria-haspopup="true"
                    aria-expanded="false">
                    <i class="dp-po-icon far fa-arrows-v"></i>
                  </a>
                  <div class="dropdown-menu dropdown-menu-left" aria-labelledby="dropdownMenuLink">
                    <a class="dropdown-item" href="#">First Name</a>
                    <a class="dropdown-item" href="#">Last Name</a>
                  </div>
                </div>
              </span>
              <span class="dp-po-table-cell dp-po-table-cell-large">Email</span>
              <span class="dp-po-table-cell">Phone Number <a href="javascript:void(0)"></a></span>
              <span class="dp-po-table-cell dp-po-table-cell-large">Job Title & Company </span>
              <span class="dp-po-table-cell dp-po-table-cell-small"></span>
            </div>

            <div class="dp-po-table-row">
              <span class="dp-po-table-cell dp-po-table-cell-large">
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" /> Artem Berdyshev
                </div>
              </span>
              <span class="dp-po-table-cell dp-po-table-cell-large">artem.berdyshev@deskpro.com</span>
              <span class="dp-po-table-cell">+441234567890</span>
              <span class="dp-po-table-cell dp-po-table-cell-large">Design Lead, Deskpro </span>
              <span class="dp-po-table-cell dp-po-table-cell-small dp-po-table-message text-right">
                <a href="javascript:void(0)" data-toggle="modal" data-target="#messageMember"><i
                    class="dp-po-icon far fa-comment-alt-dots" data-toggle="tooltip" title="Send direct message"
                    data-placement="bottom"></i></a>
              </span>
            </div>

            <div class="dp-po-table-row">
              <span class="dp-po-table-cell dp-po-table-cell-large">
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" /> Bogdan M
                </div>
              </span>
              <span class="dp-po-table-cell dp-po-table-cell-large">bogdan.m@deskpro.com</span>
              <span class="dp-po-table-cell">+441234567890</span>
              <span class="dp-po-table-cell dp-po-table-cell-large">Developer, Deskpro </span>
              <span class="dp-po-table-cell dp-po-table-cell-small dp-po-table-message text-right">
                <a href="javascript:void(0)" data-toggle="modal" data-target="#messageMember"><i
                    class="dp-po-icon far fa-comment-alt-dots" data-toggle="tooltip" title="Send direct message"
                    data-placement="bottom"></i></a>
              </span>
            </div>

            <div class="dp-po-table-row">
              <span class="dp-po-table-cell dp-po-table-cell-large">
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" /> Colin Dunn
                </div>
              </span>
              <span class="dp-po-table-cell dp-po-table-cell-large">colin.dunn@deskpro.com</span>
              <span class="dp-po-table-cell">+441234567890</span>
              <span class="dp-po-table-cell dp-po-table-cell-large">CEO, Apple </span>
              <span class="dp-po-table-cell dp-po-table-cell-small dp-po-table-message text-right">
                <a href="javascript:void(0)" data-toggle="modal" data-target="#messageMember"><i
                    class="dp-po-icon far fa-comment-alt-dots" data-toggle="tooltip" title="Send direct message"
                    data-placement="bottom"></i></a>
              </span>
            </div>

            <div class="dp-po-table-row">
              <span class="dp-po-table-cell dp-po-table-cell-large">
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" /> Giada Bobbera
                </div>
              </span>
              <span class="dp-po-table-cell dp-po-table-cell-large">giada.bobbera@deskpro.com</span>
              <span class="dp-po-table-cell">+441234567890</span>
              <span class="dp-po-table-cell dp-po-table-cell-large">Credit manager </span>
              <span class="dp-po-table-cell dp-po-table-cell-small dp-po-table-message text-right">
                <a href="javascript:void(0)" data-toggle="modal" data-target="#messageMember"><i
                    class="dp-po-icon far fa-comment-alt-dots" data-toggle="tooltip" title="Send direct message"
                    data-placement="bottom"></i></a>
              </span>
            </div>

            <div class="dp-po-table-row">
              <span class="dp-po-table-cell dp-po-table-cell-large">
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" /> Grace Howlett
                </div>
              </span>
              <span class="dp-po-table-cell dp-po-table-cell-large">grace.howlett@deskpro.com</span>
              <span class="dp-po-table-cell">+441234567890</span>
              <span class="dp-po-table-cell dp-po-table-cell-large">Hedge fund trader </span>
              <span class="dp-po-table-cell dp-po-table-cell-small dp-po-table-message text-right">
                <a href="javascript:void(0)" data-toggle="modal" data-target="#messageMember"><i
                    class="dp-po-icon far fa-comment-alt-dots" data-toggle="tooltip" title="Send direct message"
                    data-placement="bottom"></i></a>
              </span>
            </div>

            <div class="dp-po-table-row">
              <span class="dp-po-table-cell dp-po-table-cell-large">
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" /> Hannah Scott
                </div>
              </span>
              <span class="dp-po-table-cell dp-po-table-cell-large">hannah.scott@deskpro.com</span>
              <span class="dp-po-table-cell">+441234567890</span>
              <span class="dp-po-table-cell dp-po-table-cell-large">Marketing technologist </span>
              <span class="dp-po-table-cell dp-po-table-cell-small dp-po-table-message text-right">
                <a href="javascript:void(0)" data-toggle="modal" data-target="#messageMember"><i
                    class="dp-po-icon far fa-comment-alt-dots" data-toggle="tooltip" title="Send direct message"
                    data-placement="bottom"></i></a>
              </span>
            </div>
          </div>

          <!-- Modal -->
          <div class="modal fade" id="messageMember" tabindex="-1" role="dialog" aria-labelledby="messageMember"
            aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered" role="document">
              <div class="modal-content">
                <div class="modal-header">
                  <h5 class="modal-title" id="messageMember">Send a new message</h5>
                </div>
                <div class="modal-body">
                  <form class="dp-po-form">
                    <div class="form-group">
                      <div class="dp-po-avatar dp-po-avatar-round">
                        To: &nbsp; <img src="/portal-components/portal-style/static/img/docs/avatar.png" /> Artem
                        Berdyshev
                      </div>
                    </div>
                    <div class="form-group">
                      <label for="exampleFormControlTextarea1">Message *</label>
                      <textarea class="form-control" id="exampleFormControlTextarea1"
                        placeholder="Your Message"></textarea>
                    </div>
                  </form>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-primary">Send Message</button>
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                </div>
              </div>
            </div>
          </div>
        </div>
        <%- include('_partials/_pagination.ejs'); %>

      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
</body>

</html>
