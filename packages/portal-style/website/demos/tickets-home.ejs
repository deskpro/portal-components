<%- include('_partials/_head.ejs'); %>

<body>

  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-tickets-home">
      <div class="container">
        <div class="row">
          <div class="col-sm-6">
            <div class="dp-po-title">
              <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/tickets.svg">
              <h2 class="dp-po-title-text">My Tickets</h2>
            </div>
          </div>
          <div class="col-sm-6 text-right">
            <div class="d-none d-sm-block">
              <a href="" class="btn btn-primary mb-4">Submit a ticket</a>
            </div>
          </div>
        </div>
        <div class="d-block d-sm-none">
          <div class="dp-po-post-mobile-area">
            <a href="" class="btn btn-primary">Submit a ticket</a>
            <a href="javascript:void(0)" data-toggle="modal" data-target="#ticketFilterMobile"><i
                class="far fa-filter"></i></a>
          </div>
        </div>
      </div>

      <div class="container">
        <div class="row">
          <div class="col-sm-3">
            <div class="d-none d-sm-block">
              <div class="dp-po-ticket-sidebar">

                <div class="dp-po-ticket-pages">
                  <ul class="dp-po-ticket-pages-list">
                    <li class="dp-po-ticket-pages-item">
                      <a href="" class="dp-po-ticket-pages-link">
                        <i class="dp-po-icon fal fa-envelope-open-text"></i> My tickets
                      </a>
                    </li>
                    <li class="dp-po-ticket-pages-item active">
                      <a href="" class="dp-po-ticket-pages-link">
                        <i class="dp-po-icon fal fa-closed-captioning"></i> Tickets i’m CC’d on
                      </a>
                    </li>
                    <li class="dp-po-ticket-pages-item">
                      <a href="" class="dp-po-ticket-pages-link">
                        <i class="dp-po-icon fal fa-atom-alt"></i> Techco tickets
                      </a>
                    </li>
                    <li class="dp-po-ticket-pages-item">
                      <a href="" class="dp-po-ticket-pages-link">
                        <i class="dp-po-icon fal fa-comments-alt"></i> My chats
                      </a>
                    </li>
                  </ul>
                </div>
                <%- include('_partials/_ticket-filter.ejs'); %>
              </div>
            </div>
          </div>
          <div class="col-sm-9">
            <div class="dp-po-section dp-po-block">
              <div class="row">
                <div class="col-10 col-sm-7 order-sm-2">
                  <form class="dp-po-ticket-search">
                    <input type="text" name="" placeholder="Search Tickets"></input>
                    <button type="submit"> <i class="dp-po-icon far fa-search"></i></button>
                  </form>
                </div>
                <div class="col-2 col-sm-1 text-right order-sm-3">
                  <div class="dp-po-columnfilter">
                    <a href="javascript:void(0)" class="dp-po-columnfilter-link" data-toggle="dropdown"
                      aria-haspopup="true" aria-expanded="false"><i class="dp-po-icon far fa-ballot-check"></i></a>
                    <div class="dropdown-menu dropdown-menu-right dp-po-columnfilter-dropdown">
                      <ul class="dp-po-columnfilter-list">
                        <li class="dp-po-columnfilter-item">
                          <div class="form-group dp-po-form-check-compact">
                            <input type="checkbox" class="form-check-input" id="idCheck">
                            <label class="form-check-label" for="idCheck">ID</label>
                          </div>
                        </li>
                        <li class="dp-po-columnfilter-item">
                          <div class="form-group dp-po-form-check-compact">
                            <input type="checkbox" class="form-check-input" id="SubjectCheck">
                            <label class="form-check-label" for="SubjectCheck">Subject</label>
                          </div>
                        </li>
                        <li class="dp-po-columnfilter-item">
                          <div class="form-group dp-po-form-check-compact">
                            <input type="checkbox" class="form-check-input" id="DepartmentCheck" checked>
                            <label class="form-check-label" for="DepartmentCheck">Department</label>
                          </div>
                        </li>
                        <li class="dp-po-columnfilter-item">
                          <div class="form-group dp-po-form-check-compact">
                            <input type="checkbox" class="form-check-input" id="UserCheck">
                            <label class="form-check-label" for="UserCheck">User</label>
                          </div>
                        </li>
                        <li class="dp-po-columnfilter-item">
                          <div class="form-group dp-po-form-check-compact">
                            <input type="checkbox" class="form-check-input" id="agentCheck">
                            <label class="form-check-label" for="agentCheck">Assigned agent</label>
                          </div>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
                <div class="col-12 col-sm-4 order-sm-1">
                  <div class="dp-po-ticketbadge dp-po-ticketbadge-danger">
                    <span class="dp-po-ticketbadge-number">5</span>You need to respond
                  </div>
                </div>

              </div>

              <div class="dp-po-table dp-po-table-col6 dp-po-table-ticket dp-po-table-ticket-danger mb-4">
                <%- include('_partials/_ticket-row-head.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
              </div>

              <div class="dp-po-ticketbadge dp-po-ticketbadge-neutral">
                <span class="dp-po-ticketbadge-number">8</span>Our agent will respond
              </div>

              <div class="dp-po-table dp-po-table-col6 dp-po-table-ticket dp-po-table-ticket-neutral mb-4">
                <%- include('_partials/_ticket-row-head.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
              </div>

              <div class="dp-po-ticketbadge dp-po-ticketbadge-success">
                <span class="dp-po-ticketbadge-number">80</span>Resolved
              </div>

              <div class="dp-po-table dp-po-table-col6 dp-po-table-ticket dp-po-table-ticket-success">
                <%- include('_partials/_ticket-row-head.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
                <%- include('_partials/_ticket-row.ejs'); %>
              </div>
            </div>
            <%- include('_partials/_pagination.ejs'); %>
          </div>
        </div>
      </div>
      <%- include('_partials/_ticket-filter-mobile-modal.ejs'); %>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>

</body>

</html>
