                      <div class="dp-po-avatar dp-po-avatar-round">
                        <img src="/portal-components/portal-style/static/img/docs/avatar.png" />
                        Nick Green
                      </div>
                    </a>
                    <div class="dropdown-menu dropdown-menu-left" aria-labelledby="dropdownMenuLink">
                      <a class="dropdown-item" href="#">User Profile</a>
                      <a class="dropdown-item" href="#">Send user a Direct Message</a>
                    </div>
                  </div>
                </li>
              </ul>
            </div>

            <div class="row align-items-center">
              <div class="col-sm-9">
                <%- include('_partials/_post-info.ejs'); %>
              </div>
              <div class="col-sm-3">
                <div class="d-none d-sm-block">
                  <a href="" class="btn btn-icon btn-lg btn-primary"><i class="dp-po-icon fal fa-arrow-to-bottom"></i>
