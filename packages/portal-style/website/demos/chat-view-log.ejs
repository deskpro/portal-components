<%- include('_partials/_head.ejs'); %>

<body>
  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-chat-view-log">
      <div class="container">
        <div class="row mb-4">
          <div class="col-sm-9">
            <div class="dp-po-title">
              <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/chats.svg">
              <h2 class="dp-po-title-text">Your chat with Danny Hagenes</h2>
            </div>
            <div class="row align-items-center">
              <div class="col-sm-10">
                <%- include('_partials/_post-info.ejs'); %>
              </div>
              <div class="col-sm-2">
                <%- include('_partials/_post-extra.ejs'); %>
              </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-sm-9">
            <div class="dp-po-block">
              <%- include('_partials/_chat-log.ejs'); %>
            </div>
          </div>
          <div class="col-sm-3">
            <div class="dp-po-post-meta">
              <h3 class="dp-po-post-meta-title">Assigned agent</h3>
              <p>
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" />
                  <span>Danny Hagenes</span>
                </div>
              </p>
              <h3 class="dp-po-post-meta-title">Also on this chat</h3>
              <p>
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png" />
                  <span>Zac Prudent</span>
                </div>
              </p>
            </div>
            <div class="dp-po-post-meta">
              <h3 class="dp-po-post-meta-title">Department</h3>
              <p>Support</p>
              <h3 class="dp-po-post-meta-title">Total chat time</h3>
              <p>9 minutes</p>
              <h3 class="dp-po-post-meta-title">Last activity</h3>
              <p>Tuesday, 6.17 pm</p>
            </div>

            <%- include('_partials/_post-attachments.ejs'); %>

            <%- include('_partials/_post-links.ejs'); %>
          </div>
        </div>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
</body>

</html>
