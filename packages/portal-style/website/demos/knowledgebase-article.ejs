<%- include('_partials/_head.ejs'); %>

<body>
  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-knowledgebase-article">
      <div class="container">

        <div class="row align-items-end mb-4">
          <div class="col-sm-9">

            <div class="dp-po-title">
              <img class="dp-po-title-svg"
                src="/portal-components/portal-style/static/img/page-icons/knowledgebase.svg">
              <h2 class="dp-po-title-text">Deskpro for IT teams</h2>
            </div>

            <div class="row align-items-end">
              <div class="col-sm-10">
                <%- include('_partials/_post-avatars.ejs'); %>
                <%- include('_partials/_post-info.ejs'); %>
              </div>
              <div class="col-sm-2">
                <div class="d-none d-sm-block">
                  <%- include('_partials/_post-extra.ejs'); %>
                </div>
              </div>
            </div>

          </div>
          <div class="col-sm-3">
            <div class="d-none d-sm-block">
              <%- include('_partials/_subscribe.ejs'); %>
            </div>
          </div>
        </div>

        <div class="d-block d-sm-none">
          <div class="dp-po-post-mobile-area">
            <%- include('_partials/_subscribe.ejs'); %>
            <a href="javascript:void(0)" data-toggle="modal" data-target="#postExtraMobile"><i
                class="far fa-ellipsis-v"></i></a>
          </div>
        </div>


        <div class="row">
          <div class="col-sm-9">

            <div class="dp-po-section">

              <div class="alert alert-icon alert-danger" role="alert">
                <i class="dp-po-icon fal fa-calendar-alt"></i> This article will unpublish on 1 July 2019.
              </div>
              <div class="alert alert-icon alert-info" role="alert">
                <i class="dp-po-icon fal fa-calendar-alt"></i> This article will unpublish on 1 July 2019.
              </div>
              <div class="alert alert-icon alert-success" role="alert">
                <i class="dp-po-icon fal fa-calendar-alt"></i> This article will unpublish on 1 July 2019.
              </div>
              <div class="dp-po-post-content dp-po-block">

                <%- include('_partials/_post-content.ejs'); %>

                <div class="dp-po-kb-attachments">
                  <h4 class="dp-po-kb-attachments-title">Attachments</h4>
                  <ul class="dp-po-kb-attachments-list">
                    <li class="dp-po-kb-attachments-item">
                      <a href="" class="dp-po-kb-attachments-link">
                        <i class="dp-po-icon fal fa-file-pdf"></i> craft-sync-user-guide.pdf
                      </a>
                    </li>
                    <li class="dp-po-kb-attachments-item">
                      <a href="" class="dp-po-kb-attachments-link">
                        <i class="dp-po-icon fal fa-file-word"></i> craft-sync-photoshop-help.docx
                      </a>
                    </li>
                    <li class="dp-po-kb-attachments-item">
                      <a href="" class="dp-po-kb-attachments-link">
                        <i class="dp-po-icon fal fa-file-excel"></i> craft-sync-photoshop-help.xlsx
                      </a>
                    </li>
                  </ul>
                </div>
              </div>
            </div>
            <%- include('_partials/_post-rating.ejs'); %>

            <%- include('_partials/_comments-thread.ejs'); %>

            <%- include('_partials/_comments-add.ejs'); %>

          </div>
          <div class="col-sm-3">

            <div class="dp-po-contents">
              <h3 class="dp-po-contents-title">Contents</h3>
              <ul class="dp-po-contents-list">
                <li class="dp-po-contents-item">
                  <a href="" class="dp-po-contents-link">Deskpro for IT Teams</a>
                </li>
                <li class="dp-po-contents-item">
                  <a href="" class="dp-po-contents-link active">Creating Effective New Ticket Contact Forms</a>
                </li>
                <li class="dp-po-contents-item">
                  <a href="" class="dp-po-contents-link">How do I get started with Deskpro?</a>
                </li>
                <li class="dp-po-contents-item">
                  <a href="" class="dp-po-contents-link">What documentation is available for Deskpro?</a>
                </li>
                <li class="dp-po-contents-item">
                  <a href="" class="dp-po-contents-link">Setting up a Custom Email Address with Deskpro Cloud</a>
                </li>
              </ul>
            </div>

            <%- include('_partials/_post-label.ejs'); %>

            <%- include('_partials/_related-content.ejs'); %>

            <%- include('_partials/_post-share.ejs'); %>

            <%- include('_partials/_post-meta.ejs'); %>
          </div>
        </div>

        <!-- Mobile Modal -->
        <div class="modal fade mobile-modal" id="postShareMobile" tabindex="-1" role="dialog"
          aria-labelledby="postShareMobile" aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body">
                <%- include('_partials/_post-share-mobile.ejs'); %>
              </div>
            </div>
          </div>
        </div>

        <!-- Mobile Modal -->
        <div class="modal fade mobile-modal" id="postExtraMobile" tabindex="-1" role="dialog"
          aria-labelledby="postExtraMobile" aria-hidden="true">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                  <span aria-hidden="true">&times;</span>
                </button>
              </div>
              <div class="modal-body">
                <%- include('_partials/_post-extra-mobile.ejs'); %>
              </div>
            </div>
          </div>
        </div>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
</body>

</html>
