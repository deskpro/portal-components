<%- include('_partials/_head.ejs'); %>

<body>
  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-news-post">
      <div class="container">
        <div class="row align-items-end mb-4">
          <div class="col-sm-9">
            <div class="dp-po-news-head">
              <div class="dp-po-news-head-left">
                <%- include('_partials/_post-day.ejs'); %>
              </div>
              <div class="dp-po-news-head-right">
                <div class="dp-po-title">
                  <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/news.svg">
                  <h2 class="dp-po-title-text">Deskpro 5.14.0, 5.14.1, 5.14.2, Released</h2>
                </div>
                <div class="row align-items-end">
                  <div class="col-sm-10">
                    <%- include('_partials/_post-avatars.ejs'); %>
                    <%- include('_partials/_post-info.ejs'); %>
                  </div>
                  <div class="col-sm-2">
                    <div class="d-none d-sm-block">
                      <%- include('_partials/_post-extra.ejs'); %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="col-sm-3">
            <div class="d-none d-sm-block">
              <%- include('_partials/_subscribe.ejs'); %>
            </div>
          </div>
        </div>
        <div class="d-block d-sm-none">
          <div class="dp-po-post-mobile-area">
            <%- include('_partials/_subscribe.ejs'); %>
            <a href="javascript:void(0)" data-toggle="modal" data-target="#postExtraMobile"><i
                class="far fa-ellipsis-v"></i></a>
          </div>
        </div>
        <div class="row">
          <div class="col-sm-9">

            <div class="dp-po-section">

              <div class="dp-po-post-content dp-po-block">
                <%- include('_partials/_post-content.ejs'); %>
              </div>
            </div>

            <%- include('_partials/_post-rating.ejs'); %>

            <%- include('_partials/_comments-thread.ejs'); %>

            <%- include('_partials/_comments-add.ejs'); %>

          </div>
          <div class="col-sm-3">
            <%- include('_partials/_related-content.ejs'); %>

            <div class="d-none d-sm-block">
              <%- include('_partials/_post-share.ejs'); %>
            </div>

            <%- include('_partials/_post-label.ejs'); %>

            <div class="dp-po-sidebar dp-po-news-recent">
              <h3 class="dp-po-sidebar-title d-none d-sm-block">Recent News</h3>
              <h3 class="dp-po-sidebar-title d-sm-none dp-po-mobile-collapse" data-toggle="collapse"
                data-target="#collapseOne"> Recent News <i class="dp-po-icon fal fa-angle-up"></i></h3>
              <div id="collapseOne" class="collapse show">
                <ul class="dp-po-news-recent-list">
                  <li class="dp-po-news-recent-item ">
                    <a href="" class="dp-po-news-recent-link">
                      <span class="dp-po-news-recent-link-date">Jun 08</span>
                      Logging tasks and tracking projects
                    </a>
                  </li>
                  <li class="dp-po-news-recent-item">
                    <a href="" class="dp-po-news-recent-link">
                      <span class="dp-po-news-recent-link-date">Dec 10</span>
                      Easily track and respond to incidents from the help desk
                    </a>
                  </li>
                  <li class="dp-po-news-recent-item active">
                    <a href="" class="dp-po-news-recent-link">
                      <span class="dp-po-news-recent-link-date">Jun 31</span>
                      Deskpro 5.14.0, 5.14.1, 5.14.2, Released
                    </a>
                  </li>
                  <li class="dp-po-news-recent-item">
                    <a href="" class="dp-po-news-recent-link">
                      <span class="dp-po-news-recent-link-date">Mar 11</span>
                      Auto-assignment - route requests to the most relevant team or technician
                    </a>
                  </li>
                  <li class="dp-po-news-recent-item">
                    <a href="" class="dp-po-news-recent-link">
                      <span class="dp-po-news-recent-link-date">Jun 01</span>
                      Creating an effective, customized contact form for users to submit requests to
                    </a>
                  </li>
                </ul>
                <div class="dp-po-viewall">
                  <a class="dp-po-viewall-link" href="">View all news <i class="dp-po-icon far fa-angle-right"></i></a>
                </div>
              </div>
            </div>

          </div>
        </div>
        <%- include('_partials/_post-extra-mobile-modal.ejs'); %>
        <%- include('_partials/_post-share-mobile-modal.ejs'); %>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
</body>

</html>
