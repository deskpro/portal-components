<%- include('_partials/_head.ejs'); %>

<body>
  <%- include('_partials/_header.ejs', { loggedIn: false }); %>
  <%- include('_partials/_search.ejs'); %>
  <%- include('_partials/_breadcrumbs.ejs'); %>

  <div class="dp-po">
    <main class="dp-po-main dp-po-ticket-view">
      <div class="container">
        <div class="row">
          <div class="col-sm-9">

            <div class="row">
              <div class="col-sm-10">
                <div class="dp-po-title">
                  <img class="dp-po-title-svg" src="/portal-components/portal-style/static/img/page-icons/tickets.svg">
                  <h2 class="dp-po-title-text">Craft sync not working with updated photoshop CC 2018</h2>
                </div>
              </div>
              <div class="col-sm-2">
                <div class="d-none d-sm-block">
                  <%- include('_partials/_post-extra.ejs'); %>
                </div>
              </div>
            </div>
          </div>
          <div class="col-sm-3">
            <div class="d-none d-sm-block">
              <div class="dp-po-ticket-close">
                <a href="javascript:void(0)" class="dp-po-ticket-close-btn" data-toggle="modal"
                  data-target="#closeTicketRating">
                  <span class="fa-layers">
                    <i class="dp-po-icon fad fa-envelope"></i>
                    <i class="dp-po-icon fa-inverse fas fa-circle" data-fa-transform="shrink-8"></i>
                    <i class="dp-po-icon fas fa-times" data-fa-transform="shrink-10"></i>
                  </span>
                  Close ticket
                </a>
              </div>
            </div>
            <!-- Modal SCREEN 1 : CHANGE DATA-TARGET ABOVE TO VIEW THIS-->
            <div class="modal fade" id="closeTicket" tabindex="-1" role="dialog" aria-labelledby="closeTicket"
              aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered dp-po-ticket-modal" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <div class="dp-po-ticket-modal-header">Are you sure you want to close your ticket?</div>
                  </div>
                  <div class="modal-body">
                    <p class="mb-4">Once your ticket is resolved, our agents will no longer respond.</p>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-primary">Resolve my ticket</button>
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                  </div>
                </div>
              </div>
            </div>
            <!-- Modal SCREEN 2 : DEFAULT ACTIVE IN CODE -->
            <div class="modal fade" id="closeTicketRating" tabindex="-1" role="dialog"
              aria-labelledby="closeTicketRating" aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered dp-po-ticket-modal" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <div class="dp-po-ticket-modal-header">
                      <i class="dp-po-icon fad fa-comments-alt"></i> Rate your support
                    </div>
                  </div>
                  <div class="modal-body">
                    <div class="dp-po-ticket-modal-body">
                      <div class="dp-po-ticket-modal-agent">
                        <div class="dp-po-avatar dp-po-avatar-round">
                          <img src="/portal-components/portal-style/static/img/docs/avatar.png" />
                          <strong>Nick Green</strong>
                        </div>
                        helped with your ticket “support”
                      </div>

                      <p class="mt-4">How would you rate the support you received?</p>
                      <form class="dp-po-form">
                        <div class="form-group">
                          <div id="ticket_ticket_field_7">
                            <div id="ticket_ticket_field_7_data">
                              <div class="radios">
                                <label class="title">
                                  <input type="radio" name="rating" value="8" disabled>
                                  <i></i>
                                  <span class="label">
                                    <div class="dp-po-ticket-modal-rating dp-po-ticket-modal-rating-one">
                                      <i class="dp-po-icon fad fa-star"></i>
                                      <i class="dp-po-icon fad fa-star"></i>
                                      <i class="dp-po-icon fad fa-star"></i>
                                    </div>
                                    It was not good
                                  </span>
                                </label>
                                <label class="title">
                                  <input type="radio" name="rating" value="9">
                                  <i></i>
                                  <span class="label">
                                    <div class="dp-po-ticket-modal-rating dp-po-ticket-modal-rating-two">
                                      <i class="dp-po-icon fad fa-star"></i>
                                      <i class="dp-po-icon fad fa-star"></i>
                                      <i class="dp-po-icon fad fa-star"></i>
                                    </div>
                                    It was ok
                                  </span>
                                </label>
                                <label class=" title">
                                  <input type="radio" name="rating" value="10" checked="checked">
                                  <i></i>
                                  <span class="label">
                                    <div class="dp-po-ticket-modal-rating dp-po-ticket-modal-rating-three">
                                      <i class="dp-po-icon fad fa-star"></i>
                                      <i class="dp-po-icon fad fa-star"></i>
                                      <i class="dp-po-icon fad fa-star"></i>
                                    </div>
                                    It was great
                                  </span>
                                </label>
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="form-group">
                          <label for="exampleFormControlTextarea1">Or leave a comment</label>
                          <textarea class="form-control" id="exampleFormControlTextarea1"
                            placeholder="Your comment"></textarea>
                        </div>
                      </form>
                    </div>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-primary">Submit Feedback</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="d-block d-sm-none">
          <div class="dp-po-post-mobile-area">
            <div class="dp-po-ticket-close">
              <a href="javascript:void(0)" class="dp-po-ticket-close-btn" data-toggle="modal"
                data-target="#closeTicketRating">
                <span class="fa-layers">
                  <i class="dp-po-icon fad fa-envelope"></i>
                  <i class="dp-po-icon fa-inverse fas fa-circle" data-fa-transform="shrink-8"></i>
                  <i class="dp-po-icon fas fa-times" data-fa-transform="shrink-10"></i>
                </span>
                Close ticket
              </a>
            </div>
            <a href="javascript:void(0)" data-toggle="modal" data-target="#postExtraMobile"><i
                class="far fa-ellipsis-v"></i></a>
          </div>
        </div>
        <div class="row">
          <div class="col-sm-9">
            <%- include('_partials/_ticket-block.ejs'); %>
          </div>
          <div class="col-sm-3">
            <div class="dp-po-sidebar dp-po-ticket-status">
              <h3 class="dp-po-sidebar-title d-none d-sm-block">Ticket status</h3>
              <h3 class="dp-po-sidebar-title d-sm-none dp-po-mobile-collapse" data-toggle="collapse"
                data-target="#collapseThree"> Ticket status <i class="dp-po-icon fal fa-angle-up"></i></h3>
              <div id="collapseThree" class="collapse show">
                <span class="badge badge-warning badge-icon"><i class="dp-po-icon far fa-clock"></i> Awaiting
                  user</span>
              </div>
            </div>

            <div class="dp-po-ticket-meta">
              <h3 class="dp-po-ticket-meta-title d-sm-none dp-po-mobile-collapse" data-toggle="collapse"
                data-target="#collapseTwo"> Ticket details <i class="dp-po-icon fal fa-angle-up"></i></h3>
              <div id="collapseTwo" class="collapse show">
                <h3 class="dp-po-ticket-meta-title">Ticket ID</h3>
                <p>ALVM-6732-XRKJ</p>

                <h3 class="dp-po-ticket-meta-title">Requester</h3>
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                  Nick Green
                </div>

                <h3 class="dp-po-ticket-meta-title">Assigned agent</h3>
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                  Nick Green
                </div>

                <div class="dp-po-ticket-meta-cc">
                  <h3 class="dp-po-ticket-meta-title">Also on this ticket (12)</h3>
                  <ul class="dp-po-ticket-meta-cc-list">
                    <li class="dp-po-ticket-meta-cc-item">
                      <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                      <div class="dp-po-ticket-meta-cc-title">
                        <span class="dp-po-ticket-meta-cc-name">Jonathan Jones</span>
                        <span class="dp-po-ticket-meta-cc-email">jonathan.jones@techco.com</span>
                      </div>
                      <a href="" class="dp-po-ticket-meta-cc-remove"><i class="far fa-times-circle"></i></a>
                    </li>
                    <li class="dp-po-ticket-meta-cc-item">
                      <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                      <div class="dp-po-ticket-meta-cc-title">
                        <span class="dp-po-ticket-meta-cc-name">Jonathan Jones</span>
                        <span class="dp-po-ticket-meta-cc-email">jonathan.jones@techco.com</span>
                      </div>
                      <a href="" class="dp-po-ticket-meta-cc-remove"><i class="far fa-times-circle"></i></a>
                    </li>
                    <li class="dp-po-ticket-meta-cc-item">
                      <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                      <div class="dp-po-ticket-meta-cc-title">
                        <span class="dp-po-ticket-meta-cc-name">Jonathan Jones</span>
                        <span class="dp-po-ticket-meta-cc-email">jonathan.jones@techco.com</span>
                      </div>
                      <a href="" class="dp-po-ticket-meta-cc-remove"><i class="far fa-times-circle"></i></a>
                    </li>
                    <li class="dp-po-ticket-meta-cc-item">
                      <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                      <div class="dp-po-ticket-meta-cc-title">
                        <span class="dp-po-ticket-meta-cc-name">Jonathan Jones</span>
                        <span class="dp-po-ticket-meta-cc-email">jonathan.jones@techco.com</span>
                      </div>
                      <a href="" class="dp-po-ticket-meta-cc-remove"><i class="far fa-times-circle"></i></a>
                    </li>
                    <li class="dp-po-ticket-meta-cc-item">
                      <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                      <div class="dp-po-ticket-meta-cc-title">
                        <span class="dp-po-ticket-meta-cc-name">Jonathan Jones</span>
                        <span class="dp-po-ticket-meta-cc-email">jonathan.jones@techco.com</span>
                      </div>
                      <a href="" class="dp-po-ticket-meta-cc-remove"><i class="far fa-times-circle"></i></a>
                    </li>
                    <li class="dp-po-ticket-meta-cc-item">
                      <img src="/portal-components/portal-style/static/img/docs/avatar.png">
                      <div class="dp-po-ticket-meta-cc-title">
                        <span class="dp-po-ticket-meta-cc-name">Jonathan Jones</span>
                        <span class="dp-po-ticket-meta-cc-email">jonathan.jones@techco.com</span>
                      </div>
                      <a href="" class="dp-po-ticket-meta-cc-remove"><i class="far fa-times-circle"></i></a>
                    </li>
                  </ul>

                  <a href="" class="dp-po-ticket-meta-cc-link">CC +</a>
                  <div class="dp-po-ticket-meta-cc-form">
                    <form class="do-po-form">
                      <div class="form-group">
                        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                          placeholder="Name">
                      </div>
                      <div class="form-group">
                        <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp"
                          placeholder="Email Address">
                      </div>
                      <button type="submit" class="btn btn-primary">Add</button>
                    </form>
                  </div>
                </div>

                <h3 class="dp-po-ticket-meta-title">Organization</h3>
                <div class="dp-po-avatar dp-po-avatar-round">
                  <img src="/portal-components/portal-style/static/img/brand/logo.jpg">
                  Techco
                </div>

                <h3 class="dp-po-ticket-meta-title">Created</h3>
                <p>12 June 2019</p>

                <h3 class="dp-po-ticket-meta-title">Last activity</h3>
                <p>Today, 09.32 am</p>
              </div>
            </div>

            <div class="dp-po-sidebar dp-po-ticket-meta">
              <h3 class="dp-po-sidebar-title d-none d-sm-block">Ticket Properties</h3>
              <h3 class="dp-po-sidebar-title d-sm-none dp-po-mobile-collapse" data-toggle="collapse"
                data-target="#collapseOne"> Ticket Properties <i class="dp-po-icon fal fa-angle-up"></i></h3>
              <div id="collapseOne" class="collapse show">
                <p>
                  <a href="#" class="dp-po-sidebar-edit">Edit <i class="" data-fa-i2svg=""><svg
                        class="svg-inline--fa fa-pen fa-w-16" aria-hidden="true" focusable="false" data-prefix="fas"
                        data-icon="pen" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"
                        data-fa-i2svg="">
                        <path fill="currentColor"
                          d="M290.74 93.24l128.02 128.02-277.99 277.99-114.14 12.6C11.35 513.54-1.56 500.62.14 485.34l12.7-114.22 277.9-277.88zm207.2-19.06l-60.11-60.11c-18.75-18.75-49.16-18.75-67.91 0l-56.55 56.55 128.02 128.02 56.55-56.55c18.75-18.76 18.75-49.16 0-67.91z">
                        </path>
                      </svg></i></a>
                </p>

                <h3 class="dp-po-ticket-meta-title">Department</h3>
                <p>Support</p>
              </div>
            </div>

            <%- include('_partials/_post-attachments.ejs'); %>

          </div>
        </div>
        <%- include('_partials/_post-extra-mobile-modal.ejs'); %>
      </div>
    </main>
  </div>
  <%- include('_partials/_footer.ejs'); %>
</body>

</html>
