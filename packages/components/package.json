{
  "name": "@deskpro/portal-components",
  "version": "0.3.33",
  "description": "Deskpro Portal Components",
  "private": false,
  "homepage": "https://deskpro.github.io/portal-components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@deskpro/js-utils": "1.0.7",
    "@deskpro/react-datepicker-hijri": "^1.5.0",
    "classnames": "^2.2.5",
    "emotion": "^9.2.6",
    "es6-promise": "^4.2.4",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "libphonenumber-js": "^1.3.1",
    "lodash": "4.17.13",
    "moment": "^2.22.1",
    "moment-hijri": "^2.0.1",
    "postcss-color-mod-function": "^3.0.3",
    "raw-loader": "^0.5.1",
    "react-dropzone": "10.1.10",
    "react-select": "^2.0.0",
    "react-textarea-autosize": "^7.0.4",
    "style": "^0.0.3",
    "url-polyfill": "^1.0.13",
    "world-countries": "^2.0.0",
    "yup": "0.24.1"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "dist": "NODE_ENV=production webpack --config config/webpack.config.dist.js",
    "lint": "eslint . --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "test": "node scripts/test.js --env=jsdom",
    "travis-test": "npm run test -- --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "precommit": "lint-staged",
    "prepare": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deskpro/portal-components.git"
  },
  "author": "DeskPRO <team@deskpro.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@svgr/webpack": "^4.3.3",
    "ajv": "^6.5.2",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "1.1.3",
    "coveralls": "^3.0.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "formik": "^1.1.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "jest-webpack-resolver": "^0.3.0",
    "lint-staged": "^7.0.5",
    "object-assign": "4.1.1",
    "postcss": "^6.0.21",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "2.0.8",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.8.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.3.2",
    "resolve": "1.8.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.4"
  },
  "peerDependencies": {
    "formik": "^1.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.8.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/tests/jest/**/?(*).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "Components(.*)$": "<rootDir>/src/Components/$1",
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
