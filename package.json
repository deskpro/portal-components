{
  "name": "root",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "babel-eslint": "^10.0.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "lerna": "7.1.1",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "dev": "lerna run dev --parallel",
    "build": "lerna run --stream build",
    "test": "lerna run --stream test",
    "lint": "eslint . --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "publish-git": "lerna publish --force-publish --skip-npm --yes",
    "publish-npm": "lerna publish --force-publish --no-git-tag-version --no-push --yes"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "eslint-plugin-risxss": "^2.1.0",
    "lint-staged": "13.2.3"
  }
}
